{
  "name": "ftejs-server",
  "private": true,
  "version": "0.0.1",
  "main": "dist/server.js",
  "scripts": {
    "clean": "git clean -dfqX -- {dist,node_modules}/** && rm -rf tsconfig.tsbuildinfo dist out",
    "build": "tsc -b && bun build.ts",
    "build:bundle": "bun build.ts",
    "build:types": "tsc -b",
    "watch": "bun --watch build.ts",
    "watch:types": "tsc -w",
    "test": "npm run build:types && vitest run"
  },
  "dependencies": {
    "fte.js-base": "3.0.0-canary.430e6b6",
    "fte.js-parser": "3.0.0-canary.430e6b6",
    "fte.js-formatter": "3.0.0-canary.430e6b6",
    "fte.js-standalone": "3.0.0-canary.430e6b6",
    "prettier": "^3.6.2",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "vitest": "^3.2.4",
    "typescript": "^5.9.2"
  }
}
