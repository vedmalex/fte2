    "chokidar": "^4.0.3",
    "commander": "^10.0.1",
    "fte.js-base": "^3.0.0-canary.75a0c24",
    "fte.js-parser": "^3.0.0-canary.75a0c24",
    "fte.js-standalone": "^3.0.0-canary.75a0c24",
    "fte.js-templates": "^3.0.0-canary.75a0c24",
    "glob": "^10.4.5",
    "lodash": "^4.17.21",
    "mem-fs": "^4.1.2",
    "mem-fs-editor": "^11.1.4",
    "ts-morph": "^27.0.0"
  },
  "gitHead": "75a0c245e949010ddef97d670052f560e5b4f1ae",
