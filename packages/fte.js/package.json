{
  "name": "fte.js",
  "version": "3.0.0-canary.1912a68",
  "bin": {
    "fte.js": "./bin/fte.js"
  },
  "description": "",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "types",
    "src",
    "bin"
  ],
  "scripts": {
    "prepublish": "echo \"Running clean for fte.js...\" && npm run clean && echo \"Clean for fte.js completed, running build...\" && npm run build && echo \"Build for fte.js completed.\"",
    "build": "npm run build:all",
    "build:cjs": "FORMAT=cjs bun build.ts",
    "build:esm": "FORMAT=esm bun build.ts",
    "build:all": "npm run build:cjs && npm run build:esm && tsc -p tsconfig.build.json",
    "watch": "tsc -w -p tsconfig.json",
    "clean": "git clean -dfqX -- {dist,node_modules}/** && rm -rf tsconfig.tsbuildinfo dist",
    "test": "vitest run",
    "test:watch": "vitest run --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "bench:run": "vitest bench",
    "dev": "bun run watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
