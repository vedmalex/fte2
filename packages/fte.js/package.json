    "fte.js-parser": "^3.0.0-rc.13",
    "fte.js-standalone": "^3.0.0-rc.13",
    "fte.js-templates": "^3.0.0-rc.13",
    "glob": "catalog:runtime",
    "lodash": "catalog:runtime",
    "mem-fs": "catalog:runtime",
    "mem-fs-editor": "catalog:runtime",
    "ts-morph": "catalog:runtime"
  },
