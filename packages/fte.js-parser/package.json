{
  "name": "fte.js-parser",
  "version": "3.0.0-canary.430e6b6",
  "description": "",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "prepublish": "echo 'Skipping prepublish for debugging' && npm run build",
    "build": "npm run build:all",
    "build:cjs": "FORMAT=cjs bun build.ts",
    "build:esm": "FORMAT=esm bun build.ts",
    "build:all": "npm run build:cjs && npm run build:esm && tsc -p tsconfig.build.json",
    "watch": "tsc -w -p tsconfig.json",
    "clean": "git clean -dfqX -- {dist,node_modules}/** && rm -rf tsconfig.tsbuildinfo dist",
    "test": "vitest run",
    "test:watch": "vitest run --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "bench:run": "vitest bench",
    "dev": "bun run watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "detect-indent": "^6.1.0",
    "fte.js-base": "^3.0.0-canary.430e6b6"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "esbuild": "^0.25.10",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "gitHead": "d295175539b5752bd66436845f495f2a1aff9c82"
}
