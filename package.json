{
  "name": "fte.js-project",
  "version": "3.0.0-rc.5",
  "description": "fast template engine that was reborn",
  "scripts": {
    "build": "bun run build:all",
    "build:all": "bun run build:packages",
    "build:packages": "npx lerna run build --ignore vscode-ftejs-lang",
    "build:fte.js": "cd packages/fte.js && bun run build",
    "build:fte.js-base": "cd packages/fte.js-base && bun run build",
    "build:fte.js-browser": "cd packages/fte.js-browser && bun run build",
    "build:fte.js-formatter": "cd packages/fte.js-formatter && bun run build",
    "build:fte.js-parser": "cd packages/fte.js-parser && bun run build",
    "build:fte.js-standalone": "cd packages/fte.js-standalone && bun run build",
    "build:fte.js-templates": "cd packages/fte.js-templates && bun run build",
    "clean": "git clean -dfqX -- ./node_modules **/{dist,build,node_modules,yarn-error.log,yarn.lock,tsconfig.tsbuildinfo,package-lock.json}/",
    "templates": "bun ./packages/fte.js/bin/fte.js bundle packages/fte.js-templates/templates packages/fte.js-templates/src --typescript",
    "templates_debug": "node --inspect-brk ./packages/fte.js/bin/fte.js bundle packages/fte.js-templates/templ packages/fte.js-templates/templ --typescript",
    "ex:node-cjs": "(cd examples/node-cjs && ./build-and-run.sh)",
    "ex:node-esm": "(cd examples/node-esm && ./build-and-run.sh)",
    "ex:node-cjs-adv:build": "(cd examples/node-cjs-advanced && ./build.sh)",
    "ex:node-cjs-adv:sync": "(cd examples/node-cjs-advanced && node run-sync.js)",
    "ex:node-cjs-adv:async": "(cd examples/node-cjs-advanced && node run-async.js)",
    "ex:node-cjs-adv:stream": "(cd examples/node-cjs-advanced && node run-stream.js)",
    "ex:node-esm-adv:build": "(cd examples/node-esm-advanced && ./build.sh)",
    "ex:node-esm-adv:sync": "(cd examples/node-esm-advanced && node --input-type=module run-sync.mjs)",
    "ex:node-esm-adv:async": "(cd examples/node-esm-advanced && node --input-type=module run-async.mjs)",
    "ex:node-esm-adv:stream": "(cd examples/node-esm-advanced && node --input-type=module run-stream.mjs)",
    "ex:serve": "node ./scripts/dev-server.js",
    "ex:demo-cjs": "(cd examples/demo-node-cjs && ./build-and-run.sh)",
    "ex:demo-esm": "(cd examples/demo-node-esm && ./build-and-run.sh)",
    "ex:demo-cjs:async": "(cd examples/demo-node-cjs && node run-async.js)",
    "ex:demo-cjs:stream": "(cd examples/demo-node-cjs && node run-stream.js)",
    "ex:demo-esm:async": "(cd examples/demo-node-esm && node --input-type=module run-async.mjs)",
    "ex:demo-esm:stream": "(cd examples/demo-node-esm && node --input-type=module run-stream.mjs)",
    "format:check": "node ./packages/fte.js-formatter/dist/cli.js --check demo/complexSample/views/*.nhtml",
    "format:write": "node ./packages/fte.js-formatter/dist/cli.js --write demo/complexSample/views/*.nhtml",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "clean:dry-run": "bun run scripts/clean.ts --dry-run",
    "clean:verbose": "bun run scripts/clean.ts --verbose",
    "test": "npx lerna run test --ignore vscode-ftejs-lang",
    "test:watch": "npx lerna run test:watch --ignore vscode-ftejs-lang",
    "test:ui": "npx lerna run test:ui --ignore vscode-ftejs-lang",
    "test:coverage": "npx lerna run test:coverage --ignore vscode-ftejs-lang",
    "bench": "bun workspaces foreach -p --filter='./packages/*' --filter='!vscode-ftejs-lang' run bench",
    "typecheck": "bun workspaces foreach -p --filter='./packages/*' --filter='!vscode-ftejs-lang' exec tsc -p tsconfig.json --noEmit",
    "ci": "bun run test && bun run lint && bun run typecheck",
    "publish": "bun run publish:packages",
    "publish:force": "bun run publish:packages -- --force-publish",
    "publish:canary": "bun run publish:packages -- --canary",
    "publish:help": "bun run publish:packages -- --help",
    "publish:packages": "node scripts/publish-with-params.js",
    "publish:extension": "node scripts/publish-extension.js",
    "catalog:analyze": "node scripts/migrate-to-catalog.js analyze",
    "catalog:migrate": "node scripts/migrate-to-catalog.js migrate",
    "deps:backup": "node scripts/dependency-compatibility-manager.js backup-and-resolve",
    "deps:restore": "node scripts/dependency-compatibility-manager.js restore",
    "deps:sync-versions": "node scripts/dependency-compatibility-manager.js sync-versions",
    "deps:help": "node scripts/dependency-compatibility-manager.js help"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vedmalex/fte2.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vedmalex/fte2/issues"
  },
  "homepage": "https://github.com/vedmalex/fte2#readme",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "catalogs": {
      "build": {
        "esbuild": "^0.25.10",
        "@types/bun": "^1.2.23",
        "vite": "^7.0.0",
        "tsx": "^4.19.2"
      },
      "dev": {
        "@types/bun": "^1.0.0",
        "@types/glob": "^9.0.0",
        "@types/jest": "^30.0.0",
        "@types/lodash": "^4.17.20",
        "@types/mem-fs": "^2.2.0",
        "@types/mem-fs-editor": "^10.0.1",
        "@typescript-eslint/eslint-plugin": "8.46.0",
        "@typescript-eslint/parser": "8.46.0",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-unicorn": "^61.0.2",
        "jest": "^30.2.0",
        "jest-config": "^30.2.0",
        "prettier": "^3.6.2",
        "ts-jest": "^29.4.4",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "typescript": "next",
        "lerna": "^9.0.0",
        "nx": "^21.6.4",
        "@types/babel__core": "^7.20.5",
        "@types/babel__generator": "^7.27.0",
        "@types/babel__preset-env": "^7.10.0",
        "@types/babel__traverse": "^7.28.0",
        "@types/node": "^20.12.8",
        "@rollup/plugin-replace": "^5.0.2",
        "@total-typescript/ts-reset": "^0.3.7",
        "del-cli": "^5.0.0",
        "rollup": "^3.17.3",
        "rollup-plugin-ts": "^3.2.0",
        "rollup-plugin-tsconfig-paths": "^1.4.0",
        "vscode-languageserver": "^9.0.1",
        "vscode-languageserver-textdocument": "^1.0.11",
        "vscode-languageclient": "^9.0.1"
      },
      "testing": {
        "@vitest/ui": "^3.2.4",
        "@testing-library/react": "^11.0.2",
        "@testing-library/jest-dom": "^6.9.1",
        "vitest": "^3.2.4",
        "typescript": "^5.9.2"
      },
      "runtime": {
        "@babel/core": "^7.28.4",
        "@babel/generator": "^7.28.3",
        "@babel/parser": "^7.28.4",
        "@babel/preset-env": "^7.28.3",
        "@babel/preset-typescript": "^7.27.1",
        "@babel/traverse": "^7.28.4",
        "@babel/types": "^7.28.4",
        "@swc/core": "^1.13.5",
        "@swc/helpers": "^0.5.17",
        "@types/source-map": "^0.5.7",
        "chokidar": "^3.6.0",
        "commander": "^10.0.1",
        "glob": "^10.0.0",
        "lodash": "^4.17.21",
        "mem-fs": "^4.1.2",
        "mem-fs-editor": "^11.1.4",
        "source-map": "^0.7.4",
        "ts-morph": "^27.0.0"
      }
    }
  },
  "gitHead": "ef0fa33d54101ca9c7c7a450fc6b7fc802a2c327",
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/bun": "^1.3.0",
    "@types/node": "^24.7.2",
    "@types/vscode": "^1.96.0",
    "esbuild": "^0.25.10",
    "lerna": "9.0.0",
    "typescript": "next",
    "vitest": "^3.2.4"
  }
}