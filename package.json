{
  "name": "fte.js",
  "version": "0.7.3",
  "description": "fast template engine that is reborn",
  "main": "dist/node/index.js",
  "scripts": {
    "test": "tslint --config tslint.json ./src/**/*.ts",
    "compile": "tsc",
    "templates": "node dist/compile_templates.js",
    "grammar": "pegjs --output ./grammar/raw.pegjs.js ./grammar/raw.pegjs",
    "genit": "npm run grammar && npm run templates"
  },
  "typescript": {
    "definition": "dist"
  },
  "typings": "dist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vedmalex/fte2.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vedmalex/fte2/issues"
  },
  "homepage": "https://github.com/vedmalex/fte2#readme",
  "devDependencies": {
    "@types/escodegen": "0.0.6",
    "@types/esprima": "^2.1.33",
    "@types/fs-extra": "0.0.33",
    "@types/glob": "^5.0.30",
    "@types/node": "^6.0.45",
    "tslint": "^3.15.1"
  },
  "dependencies": {
    "fs-extra": "^0.30.0",
    "pegjs": "^0.10.0",
    "escodegen": "^1.8.0",
    "esmangle2": "^1.0.10",
    "esprima": "^3.1.0",
    "esvalid": "^1.1.0",
    "glob": "^7.0.3"
  }
}
